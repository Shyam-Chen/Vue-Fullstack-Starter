    "@vue/test-utils": "1.0.0-beta.30",
    "babel-eslint": "8.2.6",
    "babel-jest": "24.8.0",
    "babel-loader": "8.0.5",
    "babel-plugin-dynamic-import-node": "2.2.0",
    "babel-plugin-lodash": "3.3.4",
    "codecov": "3.1.0",
    "copy-webpack-plugin": "4.6.0",
