{
  "name": "vue-starter",
  "version": "1.0.0",
  "description": "A boilerplate for HTML5, Vue, Material, Babel, Flow, and PostCSS.",
  "engines": {
    "node": "12"
  },
  "scripts": {
    "serve": "cross-env BABEL_ENV=app webpack-dev-server --progress",
    "build": "rimraf public && cross-env BABEL_ENV=app webpack --env.prod",
    "lint": "eslint 'src/**/*.{js,vue}' && stylelint 'src/**/*.{css,vue}' && flow check",
    "unit": "cross-env BABEL_ENV=test JEST_ENV=app jest \"src/.*\\.js$\" --coverage --forceExit --detectOpenHandles",
    "e2e": "cross-env BABEL_ENV=test JEST_ENV=e2e jest \"src/.*\\.js$\" --forceExit --detectOpenHandles",
    "typed": "flow-typed install --ignoreDeps dev",
    "_codecov": "codecov",
    "firebase": "firebase",
    "_netlify": "netlify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shyam-Chen/Vue-Starter.git"
  },
  "author": "Shyam Chen",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "7.4.4",
    "@mdi/js": "4.4.95",
    "@tensorflow/tfjs": "1.2.7",
    "@vue/composition-api": "0.3.2",
    "apollo-boost": "0.4.3",
    "axios": "0.19.0",
    "core-js": "3.1.4",
    "echarts": "4.2.1",
    "graphql": "14.4.2",
    "graphql-request": "1.8.2",
    "graphql-tag": "2.10.1",
    "lodash": "4.17.15",
    "lru-cache": "5.1.1",
    "raven-js": "3.27.2",
    "register-service-worker": "1.6.2",
    "vue": "2.6.10",
    "vue-analytics": "5.17.0",
    "vue-apollo": "3.0.2",
    "vue-draggable": "2.0.2",
    "vue-echarts": "4.0.3",
    "vue-i18n": "8.12.0",
    "vue-markdown": "2.2.4",
    "vue-meta": "2.3.1",
    "vue-quill-editor": "3.0.6",
    "vue-router": "3.1.1",
    "vue-socket.io": "3.0.7",
    "vuetify": "2.1.14",
    "vuex": "3.1.1",
