    "css-loader": "1.0.1",
    "cssnano": "4.1.7",
    "eslint": "6.4.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-prettier": "6.3.0",
    "eslint-import-resolver-webpack": "0.11.1",
    "eslint-plugin-flowtype": "4.3.0",
