{
  "name": "vue-play",
  "version": "1.0.0",
  "description": "A boilerplate for Vue, Material, Babel, Flow, and ReactiveX.",
  "engines": {
    "node": "10"
  },
  "scripts": {
    "start": "cross-env BABEL_ENV=app webpack-dev-server --progress",
    "build": "rimraf public && cross-env BABEL_ENV=app webpack --env.prod",
    "lint": "eslint 'src/**/*.{js,vue}' && stylelint 'src/**/*.{css,vue}'",
    "unit": "cross-env BABEL_ENV=test JEST_ENV=app jest \"src/.*\\.js$\" --coverage --forceExit",
    "e2e": "cross-env BABEL_ENV=test JEST_ENV=e2e jest \"src/.*\\.js$\" --forceExit",
    "flow": "flow",
    "typed": "flow-typed install",
    "codecov": "codecov",
    "firebase": "firebase"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shyam-Chen/Vue-Play.git"
  },
  "author": "Shyam Chen",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "7.4.4",
